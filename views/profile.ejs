    <!-- *************** side bar option :- 2  *****************  -->


        <div class="myprofile-option">
            <div class="icons">
                <div class="inner-logo">
                    <a href="/mainprofile">
                        <i class="fa-solid fa-gavel"></i></a>
                </div>
                
            </div>
            <div class="profile-name">
                <div class="main-profile-name"><a href="/mainprofile">win cash</a></div>
                <div class="arrow"><a href="/mainprofile" class="first"><i class="fa-solid fa-chevron-right"></i></a></div>
                
            </div>
        </div>

            <!-- *************** side bar option :- 3 *****************  -->


        <div class="myprofile-option">
            <div class="icons">
                <div class="inner-logo">
                    <a href="/mywallet">
