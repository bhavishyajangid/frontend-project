                        <p class="pen-icon"><i class="fa-solid fa-pen"></i></p>
                    </div>
                    <div class="input-form">
                        <input type="text" placeholder="enter your name" name="name" id="names">
                        <span class="span"><i class="fa-solid fa-check"></i></span>
                    </div>

                </div>

<!-- ************** wallet option ************* -->
                <div class="mywallet">
                    <a href="/mywallet">
                        <div class="wallet-icon"><i class="fa-solid fa-wallet"></i></div>
                    </a>
                    <a href="/mywallet">
                        <div class="wallet-name">
                            <p>my wallet</p>
                        </div>
                    </a>
                </div>

            </div>

 <!-- ************** bottom div *****************            -->
            
            <div class="bottom-div">
                <p class="complete">complete profile</p>
                
                <!-- ************* kyc div ************            -->
                
                <div class="mywallet kycdiv">
                    <a href="/kyc">
                        <div class="wallet-icon id"><i class="fa-solid fa-id-card"></i></div>
                    </a>
                    <a href="/kyc">
                        <div class="wallet-name">
                            <p>complete kyc</p>
                        </div>
                    </a>
                </div>
                
 <!-- ****************** email id *****************
                -->
                <div class="mywallet kycdiv">
                    <a href="#">
                        <div class="wallet-icon id"><i class="fa-solid fa-laptop "></i></i></div>
                    </a>
                    <a href="#">
                        <div class="wallet-name emaildiv">
                            <input type="text" name="name" placeholder="enter your email id" id="email">
                            <p class="righticon"><i class="fa-solid fa-check "></i></p> 
                            <p class="emailid">update email id</p>
                        </div>
                    </a>
                </div>
            </div>
                <div class="total-won-cash">
