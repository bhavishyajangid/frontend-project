                        <div class="icon"><i class="fa-solid fa-indian-rupee-sign"></i>
                        </div>
                        <div class="options">
                            <p class="cash">cash won</p>
                            <p class="one">0</p>
                        </div>

                        
                  
        
                   </div>

                   
                        <div class="cash-won">
                            <div class="icon battel"><i class="fa-solid fa-jet-fighter-up"></i>
                            </div>
                            <div class="options">
                                <p class="cash">battel played</p>
                                <p class="one">0</p>
                            </div>
                    </div>
                </div>
